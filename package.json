{
  "name": "gropius-vsc",
  "displayName": "Gropius VS Code Extension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.editComponentDetails",
        "title": "Edit Component Details"
      },
      {
        "command": "extension.editComponentTitle",
        "title": "Edit Component Title"
      },
      {
        "command": "extension.editComponentDescription",
        "title": "Edit Component Description"
      },
      {
        "command": "extension.editIssueDetails",
        "title": "Edit Issue Details"
      },
      {
        "command": "extension.editIssueTitle",
        "title": "Edit Issue Title"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "projectsView",
          "name": "Projects"
        },
        {
          "id": "componentDetailsView",
          "name": "Component Details"
        },
        {
          "id": "issueDetailsView",
          "name": "Issue Details"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "extension.editComponentTitle",
          "when": "viewItem == editComponentTitle"
        },
        {
          "command": "extension.editComponentDescription",
          "when": "viewItem == editComponentDescription"
        },
        {
          "command": "extension.editIssueTitle",
          "when": "viewItem == editIssueTitle"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "webpack": "webpack"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vue/compiler-sfc": "^3.5.13",
    "css-loader": "^7.1.2",
    "eslint": "^9.13.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "vue": "^3.5.13",
    "vue-loader": "^17.4.2",
    "vue-tsc": "^2.1.10",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "graphql": "^16.9.0",
    "node-fetch": "^3.3.2"
  }
}